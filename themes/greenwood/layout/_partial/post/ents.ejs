<div class="participants">
  <h2 class="project-color">ENTS WORKING ON THIS</h2>
  <div id="team" class="team">
   <% var ents = site.data.ents,
          keys = Object.keys(ents),
          key = "",
          ent = "";

    for (var i = 0; i < keys.length; i++) {
      key = keys[i];
      ent = ents[key];
      if(post[key] === true){ %>
      <div class="team-member">
        <div class="members-name">
          <p class="Sentinel name"><%- ent.name %></p>
          <a href="<%- ent.link %>" class="link-to-member"><%- ent.link.replace('http://', '') %></a>
        </div>
        <img class='lazyload participants-head ' data-src="<%- ent.img %>" >
      </div>
    <% }}; %>



  </div>
</div>
